import java.util.*;
public class Rev{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        String[] ar = s.split(" ");
        for (int i=0;i<ar.length;i++){
            for(int j=ar[i].length()-1;j>=0;j--){
                System.out.print(ar[i].charAt(j));
            }
            if(i!=ar.length-1){
                System.out.print(" ");
            }
        }
    } 
    
}
