import java.util.*;
public class Fib{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a = 0 , b = 1 , c;
        while(a < n){
            c = a+b;
            a = b;
            b = c;
        }
        if (a == n){
            System.out.println("True");
        }
        else{
            System.out.println("False");
        }
    }
}
