import java.util.*;
public class Fib{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a=0 , b=1 , c=0;
        while(n>1){
            c = a+b;
            a = b;
            b = c;
            n-=1;
        }
        System.out.println(a);
    }  
}
